<?xml version="1.0" encoding="UTF-8"?>
<urlset>
	<url>
		<loc>{{$WEBSITE}}</loc>
		<lastmod>{{'Y-m-d'|date:$smarty.now}}</lastmod>
		<changefreq>daily</changefreq>
		<priority>1.0</priority>
	</url>
	{{foreach $SiteMapTag->getDepartment() as $vo}}
