<script type="text/javascript">
  $(function ()
  {
      $('.biaoge_title li:not(.disabled)').click(function() {
          $(this).closest('.biaoge_title').find('li.biaoge_active').removeClass('biaoge_active');
          $(this).addClass('biaoge_active');
      });
	  $(".biaoge_title li").click(function(){//点击链接
		   $(".biaoge_cont .biaoge_qiehuan").css("display","none");//将main下所有的div都隐藏
		   $(".biaoge_cont .biaoge_qiehuan:eq(" + $(this).index() + ")").css("display","block"); //链接对应的div显示
		  }); 
  });
  


</script> 

