*{margin:0;padding:0;font-family:"微软雅黑";box-sizing:border-box}
::after,::before{box-sizing:border-box}
a{cursor:pointer;text-decoration:none}
a:hover{cursor:pointer;text-decoration:none}
.right{background-color:#ECEDF1;width:100%;padding:20px;min-height:700px}
.wbox{background:#fff}
.right .container{width:100%}
.right .container .bench-bar{padding-right:10px;border-bottom:1px solid #dbdbdb;padding-top:10px;}
.right .container .bench-bar .pull-left{color:#333;width:100px;line-height:50px;text-align:center;display:inline-block}
.right .container .bench-bar .active{border-bottom:3px solid #36a8dc}
.right .container .bench-bar .bench-btn{color:#fff;width:150px;line-height:40px;margin-top:5px;border-radius:2px;display:inline-block;text-align:center;background:#36a8dc;text-decoration:none}
.right .container .bench-bar .bench-btn:hover{border:0;text-decoration:none}
.right .container .contact{width:98%;margin:auto;margin-top:10px;background:#fff}
.right .top{font-size:16px;line-height:50px;padding-left:10px;padding-right:10px}
.pull-right{margin-bottom: 10px;}
.pull-right a{color:#666;margin-left:20px;text-decoration:none;display:inline-block}
.pull-right a:first-child{margin-left:0}
.pull-right a:hover{color:#36a8dc;text-decoration:none}
.pull-right a.vip-buy-btn{font-size:14px;color:#ff79a2;height:30px;line-height:28px;border-radius:4px;padding-left:10px;padding-right:10px;border:1px solid #ff79a2}
.pull-right a.vip-buy-btn:hover{color:#fff;background:#ff79a2}
.h5{float:left;width:202px;height:280px;margin-top:20px;margin-left:20px;background:#fff;border:1px solid #dbdbdb;margin-bottom: 10px;}
.h5 .bottom{width:202px;padding-left:10px;padding-right:10px;line-height:40px;height:40px;overflow:hidden;border-top:1px solid #dbdbdb}
.h5 .bottom p{display:inline-block}
.h5 .bottom p a{color:#999;text-decoration:none}
.h5 .bottom p a:hover{color:#333;text-decoration:none}
.h5 .bottom2{width:202px;padding-left:20px;padding-right:10px;line-height:40px;height:40px;border-top:1px solid #dbdbdb}
.h5 .bottom2 p{display:inline-block}
.h5 .bottom2 p a{color:#999;display:inline-block;margin-right:15px;text-decoration:none}
.h5 .bottom2 p a:hover{color:#333;text-decoration:none}
.h5 .cover{position:static}
.h5 .cover img{width:200px;height:200px;display:block;cursor:pointer}
.h5 .cover .title{color:#fff;display:block;width:200px;height:30px;margin-top:-30px;position:relative;font-size:14px;line-height:30px;background:url(../image/bg.png) repeat}
.h5 .cover .choice{display:none}
.h5 .cshow .choice{display:block;width:200px;height:200px;position:relative;padding-top:20px;margin-top:-200px;background:url(../image/bg.png) repeat}
.h5 .cshow .choice .cbtn{color:#fff;width:150px;height:40px;margin:auto;display:block;line-height:40px;text-align:center;margin-bottom:20px}
.h5 .cshow .choice .cbtn1{background:#69c500}
.h5 .cshow .choice .cbtn2{background:#fb9d15}
.h5 .cshow .choice .cbtn3{background:#39a2e4}
.page{margin:10px 40px;text-align:center;font-size:14px;font-family:verdana,arial,helvetica,sans-serif;line-height:40px}
.page a{margin:2px;height:40px;color:#36a8dc;border:#36a8dc 1px solid;display:inline-block;text-decoration:none}
.page a.num{width:40px}
.page a.end,.page a.first,.page a.next,.page a.prev{width:60px}
.page a:hover{color:#fff;background-color:#36a8dc}
.page a:active{color:#fff;border:#36a8dc 1px solid;background-color:#36a8dc}
.page span{margin:2px;width:40px;height:40px;color:#36a8dc;border:#36a8dc 1px solid;background-color:#36a8dc;display:inline-block}
.page span.current{color:#fff}
.breadcrumb_h5{background-color:#f5f5f5;border-radius:4px;list-style:outside none none;border-bottom:1px solid #e5e5e5;height:39px;line-height:39px;overflow:hidden;padding-left:20px}
/* Bootstrap */
.glyphicon{position:relative;top:1px;display:inline-block;font-family:'Glyphicons Halflings';font-style:normal;font-weight:400;line-height:1;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale}
.glyphicon-plus:before{content:"\2b"}
.row{margin-right:-15px;margin-left:-15px}
.clearfix::after,.clearfix::before,.container::after,.container::before,.row::after,.row::before{display:table;content:" "}
.col-md-12{width:100%;position:relative;min-height:1px;padding-right:15px;padding-left:15px;float:left}
.row::after,.clearfix::after,.container::after{clear:both}
.pull-right{float:right!important}
.container{padding-right:15px;padding-left:15px;margin-right:auto;margin-left:auto}
@media (min-width:768px){.container{width:750px}
}
@media (min-width:992px){.container{width:970px}
}
@media (min-width:1200px){.container{width:1170px}
}
/*导航条*/
.mainnav{z-index:1;background-color:#222}/*导航条背景*/
.mainnav > ul{ font-size: 0; line-height: 0}
.mainnav > ul > li,.mainnav > ul > li > a{height:40px;line-height:40px}/*导航条高度*/
.mainnav > ul > li{display:inline-block;color:#fff;font-size:14px;font-weight:bold}/*设置字体*/
.mainnav > ul > li > a{display:inline-block;padding:0 20px;color:#fff;text-align:center}/*链接颜色*/
.mainnav > ul > li > a:hover,.mainnav > ul > li.current > a{color:#fff;text-decoration:none; background-color:#e74c3c;-webkit-transition: background-color 0.3s ease 0s; -moz-transition: background-color 0.3s ease 0s; -o-transition: background-color 0.3s ease 0s; -ms-transition: background-color 0.3s ease 0s;transition: background-color 0.3s ease 0s}