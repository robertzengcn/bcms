/*短信认证*/
.sms_rz{ overflow:hidden;}
.smsrz_tit{overflow:hidden; margin-bottom:30px;}
.smsrz_tit span{ width:60px; height:60px; text-align:center; line-height:60px; display:block; color:#fff;background-color:#429842; margin-right:10px;}
.smsrz_tit i{ font-size:40px;}
.smsrz_tit strong{ font-size:24px; color:#429842; line-height:60px;}
.smsrz_js{ margin-bottom:30px;}
.smsrz_js_tit span{ width:24px; height:24px; text-align:center; line-height:24px; display:block; color:#fff; background-color:#5a98de; margin-right:6px;}
.smsrz_js_tit span.sjt2{ background-color:#dd514c; }
.smsrz_js_tit i{ font-size:18px;}
.smsrz_js_tit strong{  line-height:24px; display:block; font-size:16px;}
.smsrz_js ul{ position:relative; line-height:30px; margin-top:10px;}
.smsrz_js ul li::before{ position:absolute; left:40px;content:"●"; color:#666; margin-right:10px; font-size:10px;}
.smsrz_js ul li p{ color:#666; padding-left:56px; margin-bottom:auto; max-width:90%;}
.smsrz_js ul li a{ color:#5a98de;}

/*医院资料信息*/
.sms_info .sms_input input{ max-width:200px; margin-right:10px;}
.sms_info .sms_input span{  font-size:12px; color:#777;}
