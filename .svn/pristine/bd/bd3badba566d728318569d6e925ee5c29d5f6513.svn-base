<title>科室医生管理-预约挂号</title>
<style type="text/css">	
	.cliinicitem_div h3{background-color: #efefef;padding: 5px 10px;font-size:16px;font-weight:bolder;height:30px;line-height:30px;}
	.clinic_item{display:block;width:72px;float:left;overflow:hidden;height:30px;line-height:30px;background:#FFFFFF;padding:5px 10px;font-size:14px;}	
</style>
</head>
<body>
<div class="pd-20">
	<form action="" method="post" class="form form-horizontal" id="">
		<div id="tab-category" class="HuiTab">
			<div class="tabBar cl"><span>已有医生</span><span id="newdoctor">迁入医生</span></div>
			<div class="tabCon">				
				<div class="mt-5" id="grid">
					<table class="table table-border table-bordered table-bg table-hover table-sort" id="doctortodepartment">
						<thead>
							<tr class="text-c">
								<th width="25"><input type="checkbox" name="" value=""></th>
								<th width="60">序号</th>
								<th width="90">姓名</th>
								<th width="100">头像</th>
								<th class="text-c">个人简介</th>								
								<th width="80">操作</th>
							</tr>
						</thead>
						<tbody class="text-c">
							
						</tbody>
					</table>
				</div>
				<div style="width:100%;height:50px;clear:both;"></div>
				<div class="row cl ps-b-5 pd-5 bg-1 bk-gray">					
					<button class="btn btn-danger radius bt-normal" type="button" id="moveout"><i class="Hui-iconfont">&#xe6de;</i> 批量迁出</button>	
				</div>
			</div>
			<!--己有医生-->
			<div class="tabCon">
				<div class="pd-5">					
					<div class="cl pd-5 bg-1 bk-gray"><span class="r">医生名称：<input type="text" name="name" id="keywordin" placeholder=" 请输入关键词..." style="width:150px" class="input-text"> <button name="" id="searchbu" class="btn btn-success patientmanage" type="button"><i class="Hui-iconfont">&#xe665;</i> 搜索</button></span> </div>
					<div class="cl mt-5 col-12">						
						<div class="cliinicitem_div cl">						
							<ul id="doctor_list" class="clinic-list col-12"></ul>
						</div>	
					</div>			
				</div>
				<div style="width:100%;height:50px;clear:both;"></div>
				<div class="row cl ps-b-5 pd-5 bg-1 bk-gray">
					<button  class="btn btn-success radius bt-normal" type="button" id="chooseitem"><i class="Hui-iconfont">&#xe6dc;</i> 迁入科室</button>				
				</div>
			</div>
			
			<!--新增医生-->			
		</div><!--HuiTab over-->		
	</form>
</div>

<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
