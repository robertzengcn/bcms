<title>新增页面SEO设置</title>
</head>
<body>
<div class="pd-20">
	<form action="" method="post" class="form form-horizontal" id="formEdit">
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>页面名称：</label>
			<div class="formControls col-8">
			    <input type="hidden" id="id" name="id" value="">
				<input type="text" class="input-text" value="" placeholder="请输入页面的名称" id="page_name" name="page_name">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>文件保存目录：</label>
			<div class="formControls col-8">
				<input type="text" class="input-text" value="" placeholder="该栏目保存文件的目录名..." id="" name="">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2">SEO标题：</label>
			<div class="formControls col-8">
				<input type="text" class="input-text" value="" placeholder="" id="title" name="title">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2">关键词：</label>
			<div class="formControls col-8">
				<input type="text" class="input-text" value="" placeholder="关键词间用“,”隔开，三个以内" id="keywords" name="keywords">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2">SEO描述：</label>
			<div class="formControls col-8">
				<textarea name="description" cols="" rows="" class="textarea"  placeholder="请输入页面的内容描述..." datatype="*10-200" dragonfly="true" nullmsg="" onKeyUp="textarealength(this,200)"></textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
		</div>

		<div class="row cl">
			<div class="col-10 col-offset-2">
				<button class="btn btn-primary radius bt-normal" type="button" id="save"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button class="btn btn-default radius bt-normal" type="button" id="back">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>
	</form>
</div>
<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
<script type="text/javascript" src="../lib/layer/1.9.3/layer.js"></script> 
<script type="text/javascript" src="../lib/My97DatePicker/WdatePicker.js"></script> 
<script type="text/javascript" src="../lib/icheck/jquery.icheck.min.js"></script> 
<script type="text/javascript" src="../lib/Validform/5.3.2/Validform.min.js"></script> 
<script type="text/javascript" src="../lib/webuploader/0.1.5/webuploader.min.js"></script> 
<script type="text/javascript" src="../js/H-ui.js"></script> 
<script type="text/javascript" src="../js/H-ui.admin.js"></script> 
<script type="text/javascript" src="../js/boyicms/common.js"></script>
<script type="text/javascript" src="../js/boyicms/seo.js"></script>
<script type="text/javascript">
var gSeo = new Seo();
gSeo.setThisName('gSeo');
gSeo.initEdit();

</script>
</body>
</html>