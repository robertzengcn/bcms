<title>文件信息详细</title>
</head>
<body>
<div class="pd-20">
	<form action="" method="" class="form form-horizontal" id="formEdit">
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>文件名称：</label>
			<div class="formControls col-92">			    
				<input type="text" class="input-text" value="" placeholder="文件名称" name="name" nullmsg="文件名称不能为空！" datatype="*2-15" errormsg="限2-15个字" />
			</div><div class="col-3"></div>
		</div>		
<!-- 		<div class="row cl"> -->
<!-- 			<label class="form-label col-2"><span class="c-red">*</span>url地址：</label> -->
<!-- 			<div class="formControls col-92"> -->
<!-- 				<input type="text" class="input-text" value="" placeholder="文件下载地址" name="flag" errormsg="只能由英文、数字和“_”、“-”组成，以字母开头" datatype="Flags" nullmsg="标识不能为空"/> -->
<!-- 			</div><div class="col-3"></div> -->
<!-- 		</div> -->
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>排序：</label>
			<div class="formControls col-92">
				<input type="text" class="input-text" value="" placeholder="文件排序" name="sort" errormsg="只能为整数" datatype="n" nullmsg="不能为空"/>
			</div><div class="col-3"></div>
		</div>
		<div class="row cl">			
			<label class="form-label col-2"><span class="c-red">*</span>显示：</label>
			<div class="formControls col-92 skin-minimal">
				<div class="radio-box">
					<input type="radio" name="isshow" value="0" checked="checked"/>
					<label>是</label>
				</div>
				<div class="radio-box">
					<input type="radio" name="isshow" value="1"/>
					<label>否</label>
				</div>
			</div>		
		</div>
		<input type="hidden" id="id" name="id" value="">
		<input type="hidden" id="lastname" name="lastname" value="">
		<div style="width:100%;height:10px;clear:both;"></div>
		<div class="row cl ps-b-5 pd-5 bg-1 bk-gray">
			<div class="col-10 col-offset-1">
				<button class="btn btn-primary radius bt-normal" type="button" id="save"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button class="btn btn-default radius bt-normal" type="button" id="back">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>	
	</form>
</div>

<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
