<body>
<div class="pd-20">
	<form id="formEdit" method="post" class="form form-horizontal">
		<div class="row cl">
			<!-- <label class="form-label"><span class="c-red"></span>文件名：</label> -->
			<div class="formControls col-9" id = "name">
			</div>
			<input type="hidden" id="id" name="id" value=""/>
			<div class="col-2"> </div>
		</div>
		
		<div class="row cl">
			<!-- <label class="form-label col-2">文件内容：</label> -->
			<div class="formControls" > 				
				<textarea  style='width:99%;height:550px;background:#ffffff;' id="content" name="content" datatype="*" nullmsg=""></textarea>
			</div><div class="col-2"> </div>
		</div>
		<div class="row cl">
			<div class="">
				<button class="btn btn-primary radius" type="button" id="save"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button class="btn btn-default radius" type="button" id="back">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>
	</form>
</div>
