<title>设置微信公众号信息</title>
<style type="text/css">
	.col-2{width:140px;}
</style>
</head>
<body>
<div class="pd-20">
	<form action="post" method="post" class="form form-horizontal" id="formEdit">
		<input type="hidden" id="id" name="id" value=""/>
		<input type="hidden" id="vid" name="vid" value=""/>
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>活动名称：</label>
			<div class="formControls col-7"> <span class="select-box">
				<select name="aname" class="select" id="aname">
				</select>
				</span>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>公众号appid：</label>
			<div class="formControls col-7">
				<input type="text" class="input-text" value="" placeholder="在微信公众号后台开发者中心查找。" name="appid" id="appid" datatype="*17-18" nullmsg="公众号appid不能为空" errormsg="限18个字符"/>
			</div>
			<div class="col-3"></div>
		</div>
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>公众号appsecret：</label>
			<div class="formControls col-7">
				<input type="text" class="input-text" value="" placeholder="在微信公众号后台开发者中心查找。" name="appsecret" id="appsecret" datatype="*31-32" nullmsg="公众号appsecret不能为空" errormsg="限32个字"/>
			</div>
			<div class="col-3"></div>
		</div>

		<div class="row cl ps-b-5 pd-5 bg-2 bk-gray">
			<div class="col-10 col-offset-1">
				<button class="btn btn-primary radius bt-normal" type="button" id="save"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button class="btn btn-default radius bt-normal" type="button" onClick="layer_close();">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>
	</form>
</div>
<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
<script type="text/javascript" src="../lib/icheck/jquery.icheck.min.js"></script> 
