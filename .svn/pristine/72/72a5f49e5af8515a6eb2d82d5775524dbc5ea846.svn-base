<!doctype html>
<html>
<head>
<meta charset="utf-8">
<meta name="description" content="">
<meta name="keywords" content="">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>注册</title>

<!-- Set render engine for 360 browser -->
<meta name="renderer" content="webkit">

<!-- No Baidu Siteapp-->
<meta http-equiv="Cache-Control" content="no-siteapp"/>
<link rel="icon" type="image/png" href="{{$LOGO[2]}}">

<!-- Add to homescreen for Chrome on Android -->
<meta name="mobile-web-app-capable" content="yes">
<link rel="icon" sizes="192x192" href="{{$LOGO[1]}}">

<!-- Add to homescreen for Safari on iOS -->
<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">
<meta name="apple-mobile-web-app-title" content="UI"/>
<link rel="apple-touch-icon-precomposed" href="{{$LOGO[1]}}">

<!-- Tile icon for Win8 (144x144 + tile color) -->
<meta name="msapplication-TileImage" content="{{$LOGO[1]}}">
<meta name="msapplication-TileColor" content="#0e90d2">
<!--单页样式-->
<link href="{{$COMMON}}Commodity/public/css/login.css" rel="stylesheet" type="text/css" />
<script src="{{$COMMON}}Commodity/public/js/jquery-1.7.min.js"></script>
<script src="{{$COMMON}}Commodity/public/js/register.js"></script>
</head>
<body>
 	<div style="text-align: center;position: fixed;top: 0;width: 100%;height: 100%;z-index: 100000;background-color: rgba(0,0,0,.1);display:none;" id="loading">
		<img src="{{$COMMON}}Commodity/public/image/loading.gif"  style="width: 40px;height: 40px;position: absolute;top: 50%;">
	</div> 
<div class="heght10"></div>
  <h3 class="title">商城注册</h3>
  <div class="reg"><a href="{{$METHOD}}login" >已有账号</a></div>
<div class="zc">
 <div class="zc_con1">
	<form action="" method="post" id="signup-user-form" autocomplete="off">
	   <div class="zc_list">
	   <span class="zc_bt">手<b class="kg04">机</b>号</span>
	   <input type="text" value="请输入11位手机号" id="settings-mobile" class="text03" name="mobile">
	   <div class="pwd_grade"></div>
	   </div>
	   <div class="zc_list">
	   <span class="zc_bt">验<b class="kg04">证</b>码</span>
	   <input type="text"  value="输入短信验证码" class="text03" name="sms_code" id="sms-code">
	   <input type="button" value="获取验证码" class="sendsms_button btn_disable" id="get_regsms_code">
	   <div class="pwd_grade"></div>
	   </div>
	   <div class="zc_list">
		<span class="zc_bt">姓<b class="kg05"></b>名</span>
		<input type="text"  value="请输入真实姓名" class="text03" id="signup-membername" name="membername">
	   <div class="pwd_grade"></div>
	   </div>
	   <div class="zc_list">
		<span class="zc_bt">密<b class="kg05"></b>码</span>
		<input type="password"  placeholder="6-16个字符或字母" class="text03" id="signup-password" name="user_pwd">
		<input type="hidden"  value="密码" name="td_pwdval">
	   <div class="pwd_grade"></div>
		</div>
	   <div class="zc_list">
		<span class="zc_bt">确认密码</span>
		<input type="password" placeholder="再次输入密码"  class="text03" id="signup-password-confirm" name="user_pwd_confirm">
	   <div class="pwd_grade"></div>
		</div>
 </div>
<!--   <p><input type="checkbox" checked id="J_agreement" name="agreement">我已阅读<a href="#">并同意商城《服务协议》 </a></p> -->
  <a class="zcbut"  id="signup-mobile-submit" style="margin-left:10px;">注册</a>
  </form>
</div>
 <div id="pageDialogBG" class="pageDialogBG"><div class="Prompt"></div> </div>
</body>
</html>