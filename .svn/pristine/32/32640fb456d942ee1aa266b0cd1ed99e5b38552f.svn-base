@charset "utf-8";
/* CSS Document */
/*票数榜*/
.type_box{border:1px #5677fc solid;}
div.selected{background-color:#5677fc;}
/*投票累积*/
.num_btn{background-color:#5677fc;border:1px solid #4e6cef;}
/*票数榜*/
.player_box .vote a{border: 1px solid #5677fc;color: #4e6cef;}
/*奖品*/
.prize_title .tit{background:url(../../images/blue/prize-tit-bg.png) no-repeat center center;background-size: contain;}
.prize_box{ background-color:#d0d9ff; border:1px solid #afbfff; }
/*活动简介*/
.info_title .tit{color:#4e6cef;}
.info_title i{border-bottom: #4e6cef 1px solid;}
/*排名*/
.ranking_btn{border:1px solid #4e6cef;color:#5677fc;}
.ranking_btn:hover{background-color:#5677fc;}
.ranking_box .ranking_list:nth-child(1) .ranking,.ranking_box .ranking_list:nth-child(2) .ranking,.ranking_box .ranking_list:nth-child(3) .ranking{color:#FC573D;} /*不用改颜色*/
/*选手详情*/
.details .details_top a{ background-color:#5677fc;}
.details .details_top a.details_btn{ background-color:#afbfff;}
.details .details_btn{ background-color:#5677fc;}
.details_footer a:nth-child(3){ background-image:url(../../images/blue/details_footer2.png)}
.details_footer a:nth-child(2){ border:1px solid #4e6cef;color:#5677fc;background:#afbfff url(../../images/blue/details_footer4.png) no-repeat;background-size: 26px 26px;background-position: center 8px;}
.details_footer a:nth-child(2).active{background-image:url(../../images/blue/details_footer3.png)}
.details_footer a.yes{color:#5677fc;}
.details_footer a:nth-child(1).yes{ background-image:url(../../images/blue/details_footer1.png)}
.details_footer a:nth-child(3).yes{ background-image:url(../../images/blue/details_footer2.png)}
/*上传内容*/
.upload_btn{background-color: #5677fc;}
.upload .upload_img .upload_bg_img{display: inline-block; margin: auto auto 10px; width: 100px;height: 80px;  border-radius:4px; background-image:url(../../images/blue/photo_s1.png);}
/*操作成功*/
.succeed .weui_icon_success::before{ color:#5677fc;}
.succeed .weui_btn_warn{ background-color:#5677fc;}

.diamond_number .details_footer_img{background: url(../../images/blue/details_footer2.png);}
/*钻石*/
.diamond_zf div.active{ border-color:#3cc51f;color:#3cc51f;} /*不用改颜色*/
.diamond_zf div.active:before{border-bottom: 18px solid #3cc51f;} /*不用改颜色*/
.diamond_cz li.active a{ background-image:url(../../images/blue/details_footer2.png); color:#5677fc; background-color:#ffe5ec;}
.diamond_cz li.active a span{ border-color:#5677fc;}
.diamond_cz li.active a small{ color:#5677fc;}

/*图标*/
.tabbar_icon1{background:url(../../images/blue/tabbar_icon1.png) no-repeat 0 -24px;}
.tabbar_icon2{background:url(../../images/blue/tabbar_icon2.png) no-repeat 0 -24px;}
.tabbar_icon3{background:url(../../images/blue/tabbar_icon3.png) no-repeat 0 -24px;}
.tabbar_icon4{background:url(../../images/blue/tabbar_icon4.png) no-repeat 0 -24px;}
.tabbar_icon5{background:url(../../images/blue/tabbar_icon5.png) no-repeat 0 -24px;}
.pop_up .gz_btn{color: #5677fc;}
.home_footer .weui_tabbar_item.weui_bar_item_on .weui_tabbar_label{color: #5677fc;}
  /*弹窗*/
.Prompt s,.goodsList .goodsListR a.add s,.registerCon em,.haveNot s,.z-del,.g-pay-bline,.z-pay-mentsel,.z-pay-tips s,.z-pay-tips i,.z-Transaction s,.z-Transaction i,.pCountdown s,.pCountdown em,.z-limit-tips {background:url(../../images/blue/seticon.png);background-size:23px auto;}