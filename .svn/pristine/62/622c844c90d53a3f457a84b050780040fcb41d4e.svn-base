<script type="text/javascript">
/*打开新窗口*/
function open_newindow(title,url,w,h){	
	parent.layer.open({
		type: 2,
		area: [w+'px', h +'px'],
		fix: false, //不固定
		maxmin: true,
		shade:0.4,
		title: title,
		content: url
	});
}

$(function(){
	$('div[id^="showmsg"]').on('mouseenter','a',function(){
		var msg=$(this).attr('info');
		layer.tips(msg, $(this), {
			tips: [3, '#78BA32'],
			time:15000
		});
	});

});
</script>
