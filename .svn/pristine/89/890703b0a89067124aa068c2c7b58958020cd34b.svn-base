<title>新增第三方代码</title>
</head>
<body>
<div class="pd-20">
	<form action="" method="post" class="form form-horizontal" id="formEdit">
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>代码名称：</label>
			<div class="formControls col-7">
			    <input type="hidden" id="id" name="id" value="">
				<input type="text" class="input-text" value="" placeholder="" id="subject" name="subject"nullmsg="名称不能为空" datatype="*2-50" errormsg="限2-50个字" />
			</div><div class="col-2"></div>
		</div>
		<div class="row cl">			
			<label class="form-label col-2">使用：</label>
			<div class="formControls col-92 skin-minimal"><!--add col-92 css-->
				<div class="radio-box">
					<input type="radio" id="isuse_1" name="isuse" value="1" checked="checked">
					<label for="isuse_1">是</label>
				</div>
				<div class="radio-box">
					<input type="radio" id="isuse_0" name="isuse" value="0">
					<label for="isuse_0">否</label>
				</div>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2">详细代码：</label>
			<div class="formControls col-7">
				<textarea name="js" cols="" rows="" class="textarea"  placeholder="请输入详细的代码..." datatype="*" dragonfly="true" nullmsg="代码不能为空！" style="height:260px;"></textarea>				
			</div><div class="col-2"></div>
		</div>
		<div style="width:100%;height:60px;clear:both;"></div>
		<div class="row cl ps-b-5 pd-5 bg-1 bk-gray">
			<div class="col-10 col-offset-1">
				<button class="btn btn-primary radius bt-normal" type="button" id="save"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button class="btn btn-default radius bt-normal" type="button" id="back">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>		
	</form>
</div>
<script type="text/javascript" src="../lib/jquery/1.9.1/jquery.min.js"></script> 
<script type="text/javascript" src="../lib/layer/1.9.3/layer.js"></script> 
