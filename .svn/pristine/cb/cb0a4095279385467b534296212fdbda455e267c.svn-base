<title>新增友链</title>
</head>
<body>
<div class="pd-20">
	<form action="" method="post" class="form form-horizontal" id="">
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>友链名称：</label>
			<div class="formControls col-9">
				<input type="text" class="input-text" value="" placeholder="" id="" name="">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2"><span class="c-red">*</span>对方网址：</label>
			<div class="formControls col-9">
				<input type="text" class="input-text" value="" placeholder="" id="" name="">
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-2">排序：</label>
			<div class="formControls col-94">
				<input type="text" class="input-text" value="" placeholder="" id="" name="">
			</div>			
			<label class="form-label col-2">显示：</label>
			<div class="formControls col-92 skin-minimal"><!--add col-92 css-->
				<div class="radio-box">
					<input type="radio" id="sex-1" name="sex" datatype="*" nullmsg="">
					<label for="sex-1">是</label>
				</div>
				<div class="radio-box">
					<input type="radio" id="sex-2" name="sex">
					<label for="sex-2">否</label>
				</div>
			</div>
			
		</div>
		

		<div class="row cl">
			<div class="col-10 col-offset-2">
				<button onClick="" class="btn btn-primary radius bt-normal" type="submit"><i class="Hui-iconfont">&#xe632;</i> 保存</button>				
				<button onClick="layer_close();" class="btn btn-default radius bt-normal" type="button">&nbsp;&nbsp;取消&nbsp;&nbsp;</button>
			</div>
		</div>
	</form>
</div>
