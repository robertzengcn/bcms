@charset "utf-8";
/* CSS Document */



ul,li{list-style: none;}
/*首页*/
/*头部*/
.head{ padding:30px 0; background-color: #333; width:100%;}
.fz12{ font-size:12px;}

.head .navbar-form{box-shadow:none;}
.head .navbar-form .form-group{ position:relative;width:42%;}
.head .navbar-form .form-control{ border-radius:0;}
.head .navbar-form .btn{border-radius:0;}
.head .head-operates-logined a{ float:left; margin:0 10px; color:#8A8D92;}
.head .head-operates-logined a:hover{ text-decoration:none;}
.head .head-operates-logined i{ color:#ff6d6b; font-size:16px;}

.head .navbar-form .search-history{ width:100%; background-color:#fff; display:block; position: absolute; border: 1px solid #cdcdcd; padding-bottom:10px; color:#888; z-index:1;}
.head .navbar-form .search-history a{ color:#888;}
.head .navbar-form .search-history a:hover{ color:#FC7878; text-decoration:none;}
.head .navbar-form .search-history .left{ width:50%;min-height: 115px; padding:2%;}
.head .navbar-form .search-history .right{ width:50%; border-left:1px solid #cdcdcd; padding:2%;}
.head .navbar-form .search-history .right ul{ width:100%; padding:0; list-style:none;}
.head .navbar-form .search-history .right ul li{ float:left;}
.head .navbar-form .search-history .right ul li a{display: block; border:1px solid #d9d9d9; padding:0 5px;margin-right: 10px;margin-top: 10px;}
.head .navbar-form .search-history .right ul li a:hover{ border-color:#FC7878;}

/*导航*/
.black_dh{background-color: #000; text-align:center;overflow:hidden;}
.black_dh .nav li{ float:left;width:8%;}
.black_dh a{ line-height:48px;}
.black_dh a:hover{ text-decoration:none;}
.black_dh .nav > li > a{ padding:0;color:#fff; }
.black_dh .fenlei{ width:16%;color:#fff;  background:#333;}
.black_dh ul{ width:84%;}
@media (max-width: 767px) {
	.black_dh .fenlei{ width:26%;}
	.black_dh ul{ width:66%;}
	.black_dh .nav li{width:16%;}
	
	.head .navbar-form .form-group{ float:left; width:50%}
	.head-operates-logined{margin-top: 10px;}
	.head{ padding:0;}
	}


.black_dh .popover td{ line-height:none;}
.black_dh .popover a{ color:#333;}
.black_dh .popover a:hover{ color:#FC7878;}

.black_dh .nav > li > a:focus,.black_dh .nav > li > a:hover{ color:#000;}

.fixednav {position: fixed;left: 0px;top: 0px;width: 100%;z-index: 16;}
/*banner*/
@media (min-width: 768px) {
#myNiceCarousel .carousel-inner > .item > a > img,#myNiceCarousel .carousel-inner > .item > img{ max-width:none;left: 50%;margin-left: -960px;position: relative;}}

/*卡片内容*/
.con_box .cards-condensed .card{ text-align: -moz-center; text-align:-webkit-center; text-align:-o-center;}

/*页脚*/
.footer{ margin-top:30px;}
.footer .helpfoot{border-top: 1px #d7d7d7 solid;border-bottom: 1px #d7d7d7 solid;}
.footer .helpfoot ul{ list-style:none; padding:0; margin:0;overflow:hidden;}
.footer .helpfoot li{ width:20%; margin: 20px 0px;border-right: 1px #ebebeb solid; float:left; height:74px;}
@media (max-width: 767px) {.footer .helpfoot li{ width:50%;}}
.footer .helpfoot li i{ color:#ff6d6b; font-size:40px; margin:0 10% 0 15%}
.footer .helpfoot li a{ color:#5e5e5e;}
.footer .helpfoot li a:hover{ color:#ff6d6b; text-decoration:none;}
.footer .helpfoot li h3{ margin:0; line-height:26px;}
.footer .helpfoot li p{ margin:0; line-height:24px;}

.footer2{ margin:30px 0;}
.footer2 ul{ padding:0; list-style:none; width:360px;}
.footer2 ul li{ float:left; padding:0 5px;}
.footer2 ul li a{ color:#5e5e5e;}
.footer2 ul li a:hover{ color:#ff6d6b; text-decoration:none;}

/*悬浮导航*/
.suspension{position: fixed;bottom: 10%;right: 0px;margin-top: -85.5px;}
.suspension ul{ list-style:none;}
.suspension ul li{ width:40px; height:40px; position: relative; border:1px solid #ececec; background:#fff;}
.suspension ul li:hover{ border:1px solid #ff6d6b; background:#ff6d6b;}
.suspension ul li a{ width:40px; height:40px; text-align:center; display:block; color:#c5c5c5;}
.suspension ul li:hover a{ color:#fff;}
.suspension ul li i{ font-size:24px; line-height:40px;}
.suspension ul li p{ position:absolute; width:100px; height:40px; background:rgba(0,0,0,0.5);right: 40px; top:0;line-height: 40px; color:#fff; text-align:center;}
.suspension ul li img{ position:absolute; width:100px; height:100px; background:rgba(0,0,0,0.5);right:40px; top:0;line-height: 40px; color:#fff; text-align:center; max-width:none; border:1px solid #ccc;}


/*产品内容*/
.productdetail_content .breadcrumb{margin:7px 0;}
.breadcrumb a{ color:#333;}
.breadcrumb a:hover{ color:#ff6d6b;}

.productdetail_content .productdetail_big_img,.productdetail_content .small_img_box{ padding:0; list-style:none;}
.productdetail_content .small_img_box{ overflow:hidden;}
.productdetail_content .small_img_box img { width:50px; height:50px;}
.productdetail_content .small_img_box li{ float:left; margin:0 5px;border: 2px solid #fff;} 
.productdetail_content .small_img_box .small_active{border: 2px solid #ff6d6b;} 
.productdetail_content .bianhao{ overflow:hidden; margin-bottom:10px;}
.productdetail_content .bianhao p,.productdetail_content .bianhao a{ float:left; padding:0 5px; margin:0; text-decoration:none; color:#333;}
.productdetail_content .bianhao i{ color:#ff6d6b;}

.productdetail_content_m h2{border-bottom: 1px solid #f0f0f0; line-height:43px;}
.productdetail_content_m p span{ font-size:26px; color:#ff6d6b; font-weight:bold;}


.tab li, .tab a, .tab-item{float:left;text-align:center;}
.tab li, .tab a, .tab-item{cursor:pointer;float:left;text-align:center;}
#summary-stock .dt{margin-top:4px;}
#list1{padding-left:0;}
#list1 .dt{float:left;height:18px;width:72px;}
#list1 .dd{float:left;}

#store-selector{position:relative;float:left;z-index:2;height:26px;margin-right:6px}
#store-selector dl,#store-selector dt,#store-selector dd{float:none;color:#999}
#store-selector dl{position:absolute;top:23px;z-index:1;width:358px;width:358px;padding:5px;border:1px solid #ddd;background:#fff;display:none;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#store-selector dt{padding:6px 0 10px;color:#999}
#store-selector dd{padding-bottom:5px;line-height:18px}
#store-selector a:link,#store-selector a:visited{color:#005aa0}
#store-selector a:hover,#store-selector a:active{background:#005aa0;color:#fff}
#store-selector .text{float:left;+float:none;_float:left;position:relative;top:0;z-index:2;height:23px;background:#fff;border:1px solid #CECBCE;padding:0 20px 0 4px;line-height:23px;overflow:hidden}
#store-selector .text b{display:block;position:absolute;top:0;right:0;overflow:hidden;width:17px;height:24px;background: url(../images/btn20121210.png) 0 0 no-repeat}
#store-selector .close{display:none;position:absolute;z-index:2;top:19px;left:245px;cursor:pointer;width:17px;height:17px;background: url(../images/20120418.png) no-repeat -40px 0}
#store-selector a:hover{background:#B79567;color:#fff;cursor:pointer}
#store-selector.hover .text{border-bottom:0}
#store-selector.hover .i-storeinfo{position:relative;z-index:2;}

#store-selector.hover dl{display:block}
#store-selector.hover .close{display:block}

#store-prompt{clear:left;line-height:25px}
#store-prompt strong{font-size:14px}
#store-prompt a{color:#005EAA}
*html #store-selector .i-storeinfo,*html #store-selector .i-storeinfo div{float:left}
*html #store-selector dl{left:0}
#store-selector .content{display:none;position:absolute;top:23px;left:-85px;border:1px solid #cecbce;width:340px;padding:15px;background:#fff;-moz-box-shadow:0 0 5px #ddd;-webkit-box-shadow:0 0 5px #ddd;box-shadow:0 0 5px #ddd}
#store-selector .content select{float:left;width:120px;border:1px solid #cecbce;margin-right:15px}
#store-selector .content .select3{margin-right:0}
#store-selector.hover .content,#store-selector.hover .close{display:block}
#JD-stock{position:relative;margin-bottom:0}
#JD-stock .tab{width:100%;height:25px;float:left;border-bottom:2px solid #edd28b;overflow:visible;*overflow:hidden}
#JD-stock .tab li{float:left;clear:none;height:23px;padding:1px 1px 0;border:1px solid #ddd;border-bottom:0;margin-right:3px;background-color:#fff;line-height:22px;text-decoration:none}
#JD-stock .tab .curr{*position:relative;height:25px;padding:0;border:2px solid #edd28b;border-bottom:0}
#JD-stock .tab a{position:relative;float:left;height:23px;padding:0 20px 1px 10px;line-height:23px;text-align:center;text-decoration:none;cursor:pointer;color:#005AA0;outline:0;*blr:expression(this.onFocus=this.blur())}
#JD-stock .tab a:hover{background:0;color:#005AA0}
#JD-stock .tab a i{position:absolute;right:5px;top:10px;*top:9px;display:block;width:7px;height:5px;overflow:hidden;background: url(../images/20130606B.png) no-repeat -76px -34px;opacity:.5;filter:alpha(opacity=50)}
#JD-stock .tab .curr i,#JD-stock .tab a:hover i{opacity:1;filter:alpha(opacity=100)}
#JD-stock .area-list{padding-top:5px}
#JD-stock .area-list li{float:left;width:80px;padding:2px 0 2px 15px;clear:none}
#JD-stock .area-list li a{float:left;padding:2px 4px;*padding:0 4px;color:#005aa0}
#JD-stock .area-list li a:hover{color:#fff}
#JD-stock .area-list .longer-area{width:370px}
#JD-stock .area-list .long-area{width:170px}

.yListr{font-family:"微软雅黑";}
.yListr ul{border-bottom:1px solid #dcdcdc; padding:0;margin-bottom:13px;}
.yListr ul li{height:36px;margin-bottom:23px;}
.yListr ul li p{line-height:36px;display:inline-block;}
.yListr ul li em{cursor:pointer;color:#666666;font-size:14px;display:inline-block;padding:0 10px;font-style:normal;border:1px solid #dcdcdc;line-height:34px;height:34px;}
.yListr ul li em.yListrclickem{line-height:32px;border:2px solid #e9630a;height:32px;position:relative;padding:0 9px;}
.yListr ul li em.yListrclickem i{display:block;width:15px;height:14px;background: url(../images/righbt.png) no-repeat 0 0;right:-1px;bottom:-1px;position:absolute;}


.parameter_specification_r{ margin:20px 0;}
.parameter_specification_r_title{height: 52px;line-height: 52px;border: 1px solid #dddddd; margin:0;}
.parameter_specification_r_title span{width:166px;height: 52px;font-size: 14px;cursor: pointer; display:block; float:left;border-right: 1px solid #dddddd; text-align:center;}
.parameter_specification_r_title .psr_active{height: 50px;border-bottom: 2px solid #ff6d6b;color: #ff6d6b;}
.ps_r_title_content{border: 1px solid #eee;}
.ps_r_title_content_1{padding: 0 4% 40px 4%;height: auto;}
.ps_r_title_content_1 h4{ border-left:4px solid #C0C0C0; padding-left:3px;}

table.normal,table.normal td,table.normal th { border:1px solid #d6d6d6;line-height:2em;}
table.normal th { background:#f5f5f5; }
table.normal td {padding: 1% 15px;width: 730px; }

.spinnerExample{display: inline-block;}


/*列表页*/
.select{padding:5px 10px;border:#ddd 1px solid;border-radius:4px;font-size:12px}
.select h3{}
.select li{list-style:none;padding:10px 0 5px 60px}
.select .select-list{border-bottom:#eee 1px dashed}
.select dl{zoom:1;position:relative;line-height:24px;margin: 0;}
.select dl:after{content:" ";display:block;clear:both;height:0;overflow:hidden}
.select dt{width:100px;margin-bottom:5px;position:absolute;top:0;left:-100px;text-align:right;color:#666;height:24px;line-height:24px}
.select dd{float:left;display:inline;margin:0 0 5px 5px;}
.select a{display:inline-block;white-space:nowrap;height:24px;padding:0 10px;text-decoration:none;color:#039;border-radius:2px;}
.select a:hover{color:#f60;background-color:#f3edc2}
.select .selected a{color:#fff;background-color:#f60}
.select-result dt{font-weight:bold}
.select-no{color:#999}
.select .select-result a{padding-right:20px;background:#f60 url("../images/close.gif") right 9px no-repeat}
.select .select-result a:hover{background-position:right -15px}

.liebiao{ padding:0 !important;}
.liebiao .card .card-content span{ color:#ff6d6b; font-weight:bold;font-size: 14px;font-family: arial;}
.liebiao .card-heading { color:#333;}
.liebiao .card-heading:hover{ color:#ff6d6b;}
.liebiao .card:hover{border: 1px solid #ff6d6b;}
/*购物车*/
.gwc_cg{ padding:15px;}
.gwc_cg h1{ color:#ADE13D;}

.jiesuan .jiesuan_title h3{ float:left;}
.jiesuan .jiesuan_title span{ float:right;}
.jiesuan th{ text-align:center;}
.jiesuan table{text-align: center;}
.jiesuan table th{border-top: 3px solid #ffc6c7;}
.jiesuan .table td{ vertical-align: middle;}
.jiesuan thead label{ margin:0;}
.jiesuan .goods{ text-align:left;}
.jiesuan .goods img{width: 100%;height: 100%; max-width:100px;max-height: 100px;}
.jiesuan .count{ min-width:30px;}

.jiesuan .operation span,.jiesuan .count span{ cursor:pointer;}
.jiesuan .operation span:hover{ color:#ff6d6b;}
.jiesuan .count .add,.jiesuan .count input,.jiesuan .count .reduce{ margin:2px 2px;}
.jiesuan .count .add,.jiesuan .count .reduce{border:1px solid #e5e5e5;background:#f0f0f0;text-align:center;line-height:23px;color:#444; padding:5px;}
.jiesuan .count .add:hover,.jiesuan .count .reduce:hover{color:#f50;z-index:3;border-color:#ff6d6b;cursor:pointer;}
.jiesuan .count input{width:60%;padding: 4px 0;line-height:15px;border:1px solid #aaa;color:#343434;text-align:center;background-color:#fff;z-index:2;} 

.foot{margin-top:0px;color:#666;height:48px;border:1px solid #c8c8c8;background: #fafafa;position:relative;}
.foot div, .foot a{line-height:48px;height:48px;}
.foot .select-all{width:80px;height:48px;line-height:48px;color:#666;text-align:center;}
.foot .delete{padding-left:10px;}
.foot .closing{border-left:1px solid #c8c8c8;width:103px;text-align:center;color:#fff;font-weight:bold;cursor:pointer; background-color:#ff6d6b;}

.foot .total{margin:0 20px;cursor:pointer;}
.foot  #priceTotal, .foot #selectedTotal, .foot #scoreTotal{color:red;font-family:"Microsoft Yahei";font-weight:bold;}
.foot .selected{cursor:pointer;}
.foot .selected .arrow{position:relative;top:-3px;margin-left:3px;}
.foot .selected .down{position:relative;top:3px;display:none;}
.show .selected .down{display:inline;}
.show .selected .up{display:none;}
.foot .selected:hover .arrow{color:red;}
.foot .selected-view{border:1px solid #c8c8c8;position:absolute;height:auto;background:#ffffff;z-index:9;bottom:48px;left:-1px;display:none;}
.show .selected-view{display:block;}

.foot .selected-view .arrow{font-size:16px;line-height:100%;color:#c8c8c8;position:absolute;right:330px;bottom:-9px;}
.foot .selected-view .arrow span{color:#ffffff;position:absolute;left:0px;bottom:1px;}

#selectedViewList{padding:10px 20px 10px 20px;}
#selectedViewList div{display:inline-block;position:relative;border:1px solid #ccc;margin:10px;float:left;}
#selectedViewList div img{width:100%;height:100%;margin-right:10px;float:left;}
#selectedViewList div span{display:none;color:#ffffff;font-size:12px;position:absolute;top:0px;right:0px;width:60px;height:18px;line-height:18px;text-align:center;background:#000;cursor:pointer;}
#selectedViewList div:hover span{display:block;}
#thescorealert{display:none;}
.jiesuan footer a{ color:#333;}
.jiesuan footer .pager li.active a{ background:#ff6d6b;}


