}

.create_scene .create_left .nav-tabs.nav-justified li a {
	border-bottom: 0;
	color: #5e676e;
	border-radius: 0;
	display: block;
	font-size: 14px;
	padding: 0
}

.create_scene .create_left .nav-tabs.nav-justified>.active>a {
	background-color: #f7f7f7;
	color: #333;
	border: none
}

