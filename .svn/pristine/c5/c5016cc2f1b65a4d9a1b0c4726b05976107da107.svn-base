.creat_head {
	height: 60px;
	background-color: #000;
	position: fixed;
	width: 100%;
	left: 0;
	right: 0;
	top: 0;
	z-index: 999;
}

.creat_head .creat_head_con{
	width:100%;
	left:0;
	margin-left:0px;
	position:relative}

.creat_head .creat_head_con .creat_logo,.creat_head .creat_head_con .creat_con {
	float: left;
	display: inline
}

.creat_head .creat_head_con .creat_save {
	float: right;
	padding-top: 7px
}

.creat_head .creat_head_con .creat_con ul {
	margin-left: 45px
}

.creat_head .creat_head_con .creat_con ul li {
	cursor: pointer;
	float: left;
	color: #FFF;
	width: 60px;
	text-align: center;
	height: 60px;
	line-height: 60px
}

.creat_head .creat_head_con .creat_con ul li span {
	display: block;
	padding-top: 14px;
	font-size: 12px;
	height: 60px;
	overflow: hidden
}

.creat_head .creat_head_con .creat_con ul li:hover span {
	background-color: #36a8dc
}

.creat_head .create-action {
	float: right;
	display: block;
	position: absolute;
	right: 150px;
	margin-left: 10px;
}

.creat_head .create-action ul li {
	cursor: pointer;
	float: left;
	color: #FFF;
	width: 60px;
	text-align: center;
	height: 60px;
	line-height: 60px
}

.creat_head .create-action ul li span {
	display: block;
	padding-top: 14px;
	font-size: 12px;
	height: 60px;
	overflow: hidden
}

.creat_head .create-action li.save:hover {
	background-color: #36a8dc
}

.creat_head .create-action li.publish:hover {
	background-color: #36a8dc
}

.creat_head .create-action li.quit:hover {
	background-color: #c33
}

.creat_head .create-action .create-save {
	background: url(../images/creat_edit/save.png) center 7px no-repeat
}

.creat_head .create-action .create-publish {
	background: url(../images/creat_edit/publish.png) center 7px no-repeat
}

.creat_head .create-action .create-quit {
	background: url(../images/creat_edit/quit.png) center 7px no-repeat
}

.create_scene {
	width: 100%;
	background: #666;
}

.create_scene .main {
